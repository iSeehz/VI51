package fr.utbm.vi51.model;

public final class Coordinate {
	private final int coordinatex;
	private final int coordinatey;
	
	public Coordinate(int a, int b){
		this.coordinatex = a;
		this.coordinatey = b;
	}
	
	public int getX(){
		return coordinatex;
	}
	
	public int getY(){
		return coordinatey;
	}
}
